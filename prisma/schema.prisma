// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("COCKROACH_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}

model User {
  id     String   @id @default(uuid())
  name   String
  image  String?
  email  String?  @unique
  phone  String   @unique
  dob    DateTime
  gender Gender
}
